{
    "name": "capstone",
    "version": "1.0.0",
    "description": "Capstone Design Microservices Architecture with Typescript",
    "main": "index.ts",
    "author": "luciferkala",
    "license": "MIT",
    "dependencies": {
        "@types/morgan": "^1.9.1",
        "@types/winston": "^2.4.4",
        "cross-env": "^7.0.2",
        "dotenv": "^8.2.0",
        "express": "^4.17.1",
        "jsonwebtoken": "^8.5.1",
        "module-alias": "^2.2.2",
        "morgan": "^1.10.0",
        "pg": "^8.3.3",
        "pg-hstore": "^2.3.3",
        "sequelize": "^6.3.5",
        "webpack": "^4.44.1",
        "winston": "^3.3.3",
        "winston-daily-rotate-file": "^4.5.0"
    },
    "scripts": {
        "tsc": "tsc",
        "ts-node": "ts-node -r tsconfig-paths/register --files",
        "test": "cross-env NODE_ENV=test jest --watchAll --verbose --runInBand --forceExit",
        "prod": "cross-env NODE_ENV=production nodemon --exec ts-node -r tsconfig-paths/register src/app.ts",
        "dev": "cross-env NODE_ENV=dev nodemon --exec ts-node -r tsconfig-paths/register src/app.ts",
        "build": "webpack --config webpack.config.js",
        "start": "tsc && node dist/src/app.js"
    },
    "_moduleAliases": {
        "@src": "dist/src",
        "@customTypes": "dist/customTypes",
        "@log": "dist/log"
    },
    "devDependencies": {
        "@babel/cli": "^7.11.6",
        "@babel/core": "^7.11.6",
        "@babel/preset-env": "^7.11.5",
        "@babel/preset-typescript": "^7.10.4",
        "@types/express": "^4.17.7",
        "@types/jest": "^26.0.10",
        "@types/jsonwebtoken": "^8.5.0",
        "@types/pg": "^7.14.4",
        "@types/sequelize": "^4.28.9",
        "@types/supertest": "^2.0.10",
        "@typescript-eslint/eslint-plugin": "^4.0.1",
        "@typescript-eslint/parser": "^4.0.1",
        "babel-jest": "^26.3.0",
        "babel-loader": "^8.1.0",
        "dotenv-webpack": "^2.0.0",
        "eslint": "^7.8.1",
        "eslint-config-airbnb-base": "^14.2.0",
        "eslint-config-prettier": "^6.11.0",
        "eslint-plugin-import": "^2.22.0",
        "eslint-plugin-prettier": "^3.1.4",
        "husky": ">=4",
        "jest": "^26.4.2",
        "lint-staged": ">=10",
        "nodemon": "^2.0.4",
        "prettier": "^2.1.1",
        "pretty-quick": "^3.0.0",
        "supertest": "^4.0.2",
        "ts-jest": "^26.3.0",
        "ts-loader": "^8.0.3",
        "ts-node": "^8.10.2",
        "tsconfig-paths": "^3.9.0",
        "typescript": "^3.9.7",
        "webpack-cli": "^3.3.12"
    }
}
